{
  "workbench.colorTheme": "SynthWave '84",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "workbench.iconTheme": "vscode-icons",
  "explorer.confirmDragAndDrop": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.confirmDelete": false,
  "editor.minimap.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
  },
  "workbench.preferredDarkColorTheme": "Dracula",
  "json.schemas": [],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.fontSize": 15,
  "terminal.integrated.tabs.enabled": true,
  "editor.autoClosingDelete": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
  },
  "[typescriptreact]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.typescript-language-features",
  },
  "diffEditor.maxComputationTime": 5000000,
  "editor.inlineSuggest.enabled": true,
  "diffEditor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "[css]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
  },
  "diffEditor.renderIndicators": false,
  "editor.autoClosingOvertype": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "json.schemas": [],
  "diffEditor.renderSideBySide": false,
  "diffEditor.codeLens": true,
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "css"
  ],
  "eslint.format.enable": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.formatOnPaste": true,
  "workbench.editor.splitInGroupLayout": "vertical",
  "explorer.confirmDelete": false,
  "eslint.alwaysShowStatus": true,
  "editor.largeFileOptimizations": false,
  "createreactfoldercomponent.javaScriptStyleTemplate": [
    ".$STYLE_COMPONENT_NAME {}"
  ],
  "createreactfoldercomponent.javaScriptComponentTemplate": [
    "//GLOBAL - components from npm",
    "import React from 'react';",
    "",
    "//COMPONENTS",
    "",
    "",
    "//SERVICES - api, conectors...",
    "",
    "",
    "//GLOBAL STATE - redux, env...",
    "",
    "",
    "//ASSETS - icons, images...",
    "",
    "",
    "export default function $COMPONENT_NAME() {",
    "  //GENERAL",
    "  ",
    "  ",
    "  //STATES",
    "  ",
    "  ",
    "  //REDUX - Selectors",
    "  ",
    "  ",
    "  //FUNCTIONS",
    "  ",
    "  ",
    "  //USE EFFECTS",
    "  ",
    "  ",
    "  return (",
    "      <div></div>",
    "  )",
    "}",
    ""
  ],
  "createreactfoldercomponent.javaScriptStyledComponentTemplate": [
    "//GLOBAL - components from npm",
    "import React from 'react';",
    "",
    "//STYLES",
    "import './$STYLE_COMPONENT_NAME.scss'",
    "",
    "//COMPONENTS",
    "",
    "",
    "//SERVICES - api, conectors...",
    "",
    "",
    "//GLOBAL STATE - redux, env...",
    "",
    "",
    "//ASSETS - icons, images...",
    "",
    "",
    "export default function $COMPONENT_NAME() {",
    "  //GENERAL",
    "  ",
    "  ",
    "  //STATES",
    "  ",
    "  ",
    "  //REDUX - Selectors",
    "  ",
    "  ",
    "  //FUNCTIONS",
    "  ",
    "  ",
    "  //USE EFFECTS",
    "  ",
    "  ",
    "  return (",
    "      <div></div>",
    "  )",
    "}",
    ""
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "none",
  "workbench.iconTheme": "material-icon-theme",
  "csharpextensions.useFileScopedNamespace": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.printWidth": 120,
  "prettier.tabWidth": 4,
  "html.format.endWithNewline": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "tabnine.experimentalAutoImports": true,
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "beautify.options": {},
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "workbench.colorTheme": "GitHub Dark",
  "files.autoSave": "afterDelay",
  "editor.language.brackets": [],
  "[python]": {
    "editor.formatOnType": true
  },
  "git.confirmSync": false,
  "git.autofetch": true
}